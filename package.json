{
  "name": "futhark-lsp",
  "version": "0.0.1",
  "publisher": "vscode-futhark",
  "engines": {
    "vscode": "^1.64.0"
  },
  "activationEvents": [
    "onLanguage:futhark"
  ],
  "main": "./out/extension",
  "contributes": {
    "languages": [
      {
        "id": "futhark",
        "extensions": [
          ".fut"
        ],
        "aliases": [
          "Futhark",
          "futhark"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Futhark",
      "properties": {}
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ."
  },
  "devDependencies": {
    "@types/node": "^17.0.14",
    "@types/vscode": "^1.63.2",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "vscode-languageclient": "^7.0.0"
  }
}
